{
    "\u0639\u0646\u0648\u0627\u0646": [
        "\u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a (\u0627\u0644\u0645\u0647\u0646\u0629)"
    ],
    "\u0623\u0633\u0645\u0627\u0621": [
        "\u0645\u0647\u0646\u0629 \u0627\u062c\u062a\u0645\u0627\u0639\u064a\u0629 \u0645\u0633\u062c\u0644\u0629 / \u0645\u0647\u0646\u0629 \u0627\u062c\u062a\u0645\u0627\u0639\u064a\u0629 \u0645\u0633\u062c\u0644\u0629"
    ],
    "\u0642\u0637\u0627\u0639\u0627\u062a \u0627\u0644\u0646\u0634\u0627\u0637": [
        "\u0627\u0644\u0631\u0639\u0627\u064a\u0629 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a\u0629\u060c \u0627\u0644\u062e\u062f\u0645\u0627\u062a \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a\u0629\u060c \u0627\u0644\u062d\u0643\u0648\u0645\u0627\u062a\u060c \u0627\u0644\u0635\u062d\u0629 \u0627\u0644\u0639\u0627\u0645\u0629\u060c \u0627\u0644\u0635\u062d\u0629 \u0627\u0644\u0639\u0627\u0645\u0629\u060c \u0627\u0644\u0635\u062d\u0629 \u0627\u0644\u0639\u0642\u0644\u064a\u0629\u060c \u0627\u0644\u0623\u0645\u0646 \u0648\u0627\u0644\u0633\u0644\u0627\u0645\u0629 \u0641\u064a \u0627\u0644\u0639\u0645\u0644\u060c \u0627\u0644\u0645\u0646\u0638\u0645\u0629 \u0627\u0644\u0645\u062c\u062a\u0645\u0639\u064a\u0629\u060c \u063a\u064a\u0631 \u0627\u0644\u0631\u0628\u062d\u064a\u0629\u060c \u0627\u0644\u0642\u0627\u0646\u0648\u0646"
    ],
    "\u0627\u0644\u0645\u0647\u0627\u0631\u0627\u062a": [
        "\u062a\u062d\u0633\u064a\u0646 \u0627\u0644\u0628\u064a\u0626\u0629 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a\u0629 \u0648\u0627\u0644\u0631\u0641\u0627\u0647\u064a\u0629 \u0644\u0644\u0623\u0634\u062e\u0627\u0635 \u0639\u0646 \u0637\u0631\u064a\u0642 \u062a\u0641\u0639\u064a\u0644 \u0648\u062a\u0646\u0645\u064a\u0629 \u0627\u0644\u0645\u0648\u0627\u0631\u062f"
    ],
    "\u0627\u0644\u062a\u0639\u0644\u064a\u0645 \u0627\u0644\u0645\u0637\u0644\u0648\u0628": [
        "\u0627\u0644\u0628\u0643\u0627\u0644\u0648\u0631\u064a\u0648\u0633 \u0641\u064a \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a (BSW)\u060c \u0627\u0644\u0628\u0643\u0627\u0644\u0648\u0631\u064a\u0648\u0633 \u0641\u064a \u0627\u0644\u0641\u0646\u0648\u0646 (BA) \u0641\u064a \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a\u060c \u0627\u0644\u0628\u0643\u0627\u0644\u0648\u0631\u064a\u0648\u0633 \u0641\u064a \u0627\u0644\u0639\u0644\u0648\u0645 \u0641\u064a \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a (BSc) \u0623\u0648 \u062f\u0628\u0644\u0648\u0645 \u0627\u0644\u062f\u0631\u0627\u0633\u0627\u062a \u0627\u0644\u0639\u0644\u064a\u0627 \u0641\u064a \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a (PGDipSW) \u0644\u0644\u0645\u0645\u0627\u0631\u0633\u0629 \u0627\u0644\u0639\u0627\u0645\u0629\u061b \u0627\u0644\u0645\u0627\u062c\u0633\u062a\u064a\u0631 \u0641\u064a \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a (MSW)\u060c \u0627\u0644\u0645\u0627\u062c\u0633\u062a\u064a\u0631 \u0641\u064a \u0627\u0644\u0641\u0646\u0648\u0646 (MA) \u0623\u0648 \u0627\u0644\u0645\u0627\u062c\u0633\u062a\u064a\u0631 \u0641\u064a \u0627\u0644\u0639\u0644\u0648\u0645 (MSc) \u0644\u0644\u0645\u0645\u0627\u0631\u0633\u0629 \u0627\u0644\u0645\u062a\u0642\u062f\u0645\u0629\u061b \u0627\u0644\u062f\u0643\u062a\u0648\u0631\u0627\u0647 \u0641\u064a \u0627\u0644\u0639\u0645\u0644 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a (DSW) \u0623\u0648 \u0627\u0644\u062f\u0643\u062a\u0648\u0631\u0627\u0647 \u0627\u0644\u0645\u0647\u0646\u064a (ProfD \u0623\u0648 DProf) \u0644\u0645\u0645\u0627\u0631\u0633\u0629 \u0645\u062a\u062e\u0635\u0635\u0629."
    ],
    "\u0645\u062c\u0627\u0644\u0627\u062a \u0627\u0644\u0639\u0645\u0644": [
        "\u062e\u062f\u0645\u0627\u062a \u062d\u0645\u0627\u064a\u0629 \u0627\u0644\u0623\u0637\u0641\u0627\u0644 \u0648\u0627\u0644\u0646\u0633\u0627\u0621\u060c \u0627\u0644\u0645\u0646\u0638\u0645\u0627\u062a \u063a\u064a\u0631 \u0627\u0644\u0631\u0628\u062d\u064a\u0629\u060c \u0627\u0644\u0645\u0646\u0638\u0645\u0627\u062a \u0627\u0644\u062d\u0643\u0648\u0645\u064a\u0629\u060c \u0645\u0631\u0627\u0643\u0632 \u0627\u0644\u0645\u062c\u0645\u0648\u0639\u0627\u062a \u0627\u0644\u0645\u0641\u0642\u0648\u062f\u0629\u060c \u0627\u0644\u0645\u0633\u062a\u0634\u0641\u064a\u0627\u062a\u060c \u0627\u0644\u0645\u062f\u0627\u0631\u0633\u060c \u0627\u0644\u0643\u0646\u0627\u0626\u0633\u060c \u0627\u0644\u0645\u0631\u0627\u0641\u0642\u060c \u0627\u0644\u0645\u0646\u0638\u0645\u0627\u062a \u0627\u0644\u0645\u062c\u062a\u0645\u0639\u064a\u0629\u060c \u0627\u0644\u062a\u062e\u0637\u064a\u0637 \u0627\u0644\u0627\u062c\u062a\u0645\u0627\u0639\u064a\u060c \u0645\u0631\u0627\u0643\u0632 \u0627\u0644\u062a\u0641\u0643\u064a\u0631\u060c \u0627\u0644\u062e\u062f\u0645\u0627\u062a \u0627\u0644\u0625\u0635\u0644\u0627\u062d\u064a\u0629"
    ]
}